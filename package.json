{
  "name": "@kartverket/kvib",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run build --filter=./packages/* && changeset version && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "turbo": "^2.1.2"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "chromatic": "^11.10.2",
    "date-fns": "^4.1.0",
    "react-day-picker": "^9.0.9"
  },
  "packageManager": "npm@9.5.0",
  "lint-staged": {
    "*.{ts,tsx,md}": "prettier --write"
  },
  "overrides": {
    "axios": "^1.7.4"
  }
}
